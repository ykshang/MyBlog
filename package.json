  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "overrides": {
      "@vueuse/core": "13.3.0",
      "echarts": "6.0.0"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ],
